        +transition(all 0.5s)
        +remove-link-underlines
        +link-colors($brown, $green, $green, $brown, false)
        font-weight: 700
#tools-examples
  @media screen and (min-width: 500px)
    /* 4 columns wide on most screens
    +columns(4,5)
    +omega(5)
  position: relative
  > h2
    @extend .visuallyhidden
  article
    max-width: 100%
    .example
      border: 1px solid $blue-dark
      +border-radius
      & + footer
        font-style: italic
        +adjust-font-size-to($ms1)
        opacity: 0.6
    .syntaxhighlighter
      white-space: nowrap
      overflow: auto
      th, td, caption
        padding: 0
        .line
          padding: 0 0.5em
      .toolbar
        display: none
      .gutter, .container
        .line
          height: 24px
      .gutter
        width: 2em
        max-width: 3em
        @extend .mono
      .container
        width: auto
        code
          @extend .mono
          
          
          
/* SURVIVORS
#survivors
  position: relative
  z-index: -54
  +box-shadow(0px 2px 2px rgba($black, 0.4), 0px 8px 8px rgba($black, 0.2))
  background-color: $green
  .inner
    @extend .container
    +padding-leader(1)
    +padding-trailer(1)
    background-color: rgba($black, 0.12)
    color: $blue-ice
    +text-shadow(rgba($blue, 0.5) 1px 2px 4px)
    +box-shadow(2px 0px 2px rgba($black, 0.1px), 8px 0px 8px rgba($black, 0.05), -2px 0px 2px rgba($black, 0.1), -8px 0px 8px rgba($black, 0.05))
    > header
      +full
      +trailer
      h1
        text-align: center
        font-weight: 300
        margin-bottom: $kit-baseline
  .survivor
    @media screen and (min-width: 500px)
      /* each item 2 columns on most screens
      +columns(2)
      max-height: 240px
    +vertical-separation($kit-baseline * 2)
    position: relative
    background: adjust-color($green, $lightness: -10)
    padding: 6px
    +box-shadow(rgba($black, 0.1) 2px 1px 4px, rgba($black, 0.1) -4px 3px 8px)
    h1
      +adjust-font-size-to($ms3)
      line-height: 1em
      max-width: 240px
      @media screen and (min-width: 901px)
        /* set font size for large screens
        position: absolute
        +adjust-font-size-to($ms4)
        line-height: 1em
    &:nth-of-type(odd)
      /* left side (odd) items
      @media screen and (min-width: 500px)
        /* adjust placement for mid screens
        +alpha()
      @media screen and (min-width: 901px)
        /* adjust placement for large screens
        padding-right: 9%
      h1
        @media screen and (min-width: 500px)
          /* move text for most screens
          text-align: right
        top: 0
        right: 0
        @media screen and (min-width: 901px)
          /* rotate titles on large screens
          +transform2d(rotate(-90deg) translate(-6px, -72px))
          +transform-origin(top, right)
    &:nth-of-type(even)
      /* right side (even) items
      @media screen and (min-width: 500px)
        /* adjust placement for mid screens
        +omega()
      @media screen and (min-width: 901px)
        /* adjust placement for large screens
        padding-left: 9%
      h1
        top: 0
        left: 0
        @media screen and (min-width: 901px)
          /* rotate titles on large screens
          +transform2d(rotate(90deg) translate(6px, -72px))
          +transform-origin(top, left)
    &:last-child
      padding-bottom: 6px
          

 
/* START
#prepared
  position: relative
  z-index: -55
  color: $white
  +adjust-font-size-to($ms3)
  background:
    color: $brown
    image: image_url('dirt-320x320.jpg')
    position: top center
  @media screen and (max-width: 320px)
    /* set different backgrounds depending on the device width
    background-size: cover
    -moz-background-size: cover
  @media screen and (max-width: 500px)
    /* set different backgrounds depending on the device width
    background-image: image_url('dirt-500x500.jpg')
    background-size: cover
    -moz-background-size: cover
  @media screen and (max-width: 900px)
    /* set different backgrounds depending on the device width
    background-image: image_url('dirt-920x776.jpg')
    background-size: cover
    -moz-background-size: cover
  @media screen and (min-width: 901px)
    /* set different backgrounds depending on the device width
    background-image: image_url('dirt-1200x776.jpg')
    background-size: cover
    -moz-background-size: cover
  .inner
    @extend .container
    +padding-leader(2)
    +padding-trailer(2)
    background-color: rgba($black, 0.25)
    +box-shadow(2px 0px 2px rgba($black, 0.4), 8px 0px 8px rgba($black, 0.2), -2px 0px 2px rgba($black, 0.4), -8px 0px 8px rgba($black, 0.2))
    position: relative
  header
    position: relative
    float: left
    +box-shadow(2px 0px 2px rgba($black, 0.2), 8px 0px 8px rgba($black, 0.1))
    h1
      background: $green
      display: inline-block
      float: left
      +text-shadow(2px 2px 2px rgba($black, 0.2), 5px 8px 8px rgba($black, 0.1))
      @media screen and (max-width: 500px)
        /* font size for most screens
        +adjust-font-size-to($ms5)
      @media screen and (min-width: 500px)
        /* faux left extension for most screens
        position: relative
        &:before
          content: '\0020'
          position: absolute
          background: $green
          top: 0
          bottom: 0
          width: 9999px
          right: 100%
      @media screen and (max-width: 320px)
        /* font size for small screens
        font-size: $ms6
      .word1
        float: left
        margin-right: 0.25em
        @media screen and (min-width: 500px)
          /* lettering.js adjustment for most screens
          +adjust-font-size-to($ms4-5, 1, $ms6)
          +transform2d(rotate(-90deg) translate(-18px, 0))
          +transform-origin(50%, 50%)
          margin-right: 0
          text-align: center
          padding: 6px 28px 
          +text-shadow(-2px 2px 2px rgba($black, 0.2), -8px 5px 8px rgba($black, 0.1))
      .word2
        @media screen and (min-width: 500px)
          /* lettering.js adjustment for most screens
          margin-left: -31px
  p.download
    float: left
    padding: 12px 0 0 0
    +adjust-font-size-to($ms3, 1, $ms3)
    span
      background-color: mix($green, $yellow)
      float: left
      clear: left
      &.word1, &.word3, &.word5
        padding-left: 0.5em
        +transition(padding-left 0.5s)
      &.word2, &.word4
        clear: none
        margin-left: 0.5em
        background-color: transparent
        color: $green
    a
      +remove-link-underlines
      +link-colors($brown-dark, $blue, $brown-dark, $brown-dark, false)
    &:hover
      span
        &.word1, &.word3, &.word5
          padding-left: 1em
  footer
    +padding-leader(0.5)
    clear: both
    text-align: right
    position: relative
    .amp
      +adjust-font-size-to($ms6, 1, $ms3)
      position: relative
      top: 0.2em
    aside
      h2
        @extend .visuallyhidden
      ul
        +inline-list
        
        
 
// random letter opacity
#teasers article, 
#intro,
#learn,
#tools #tools-examples article,
#survivors .survivor,
#prepared,
  h1, h2, h3
    // position: relative
    span
      // position: relative
      // &:nth-of-type(2n-2)
      &:nth-of-type(3n-3)
        opacity: 0.7
      &:nth-of-type(5n-5)
        opacity: 0.8
      &:nth-of-type(7n-7)
        opacity: 0.9


